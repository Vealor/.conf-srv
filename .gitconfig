[user]
  name  = Vealor
  email = jakob.m.roberts@gmail.com
[core]
  whitespace    = off
  excludesfile  = ~/.gitignore
[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old  = red bold
  new  = green bold
[color "status"]
  added     = green
  changed   = yellow
  untracked = magenta
  deleted   = red
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
